{
  "appenders": [
    { "type": "console" },
    {
      "type": "file",
      "filename": "logs/log.log",
      "maxLogSize": 204800,
      "backups": 10,
      "category": "default"
    },
    {
      "type": "file",
      "filename": "logs/access.log",
      "pattern": "-yyyy-MM-dd",
      "category": "http"
    }
  ]
}